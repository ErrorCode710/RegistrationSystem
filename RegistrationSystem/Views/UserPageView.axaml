<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			   xmlns:vm="clr-namespace:RegistrationSystem.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RegistrationSystem.Views.UserPageView"
			 x:DataType="vm:UserPageViewModel"
			 >


	<DataGrid Margin="20" ItemsSource="{Binding Users}"  AutoGenerateColumns="True"
			  IsReadOnly="True" GridLinesVisibility="All" BorderThickness="1" BorderBrush="Gray"></DataGrid>
</UserControl>
